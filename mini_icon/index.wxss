/**index.wxss**/
/*字体*/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fenxiang:before { content: "\e646"; }

.icon-dianzan:before { content: "\e668"; }

.icon-huatong:before { content: "\e655"; }

.icon-shoucang:before { content: "\e660"; }



/*页面样式*/

.vedio-wrap{
  height: 300px;
  width: 100%;
  border-bottom: 1px solid #f7f7f7;
}
.clear-float::after{
  content: "";
  height: 0;
  display: block;
  visibility: hiden;
  clear: both;
}
.video-info-wrap{
  width: 100%;
  margin-top: 3px;
}
.user-image,.user-name,.vedio-time,.power,.charimas,.comment-button{
  float: left;
}
.user-image{
  /* height: 80rpx;
  width: 80rpx;
  border-radius: 25rpx; */
  width: 15%;
  height: 15vw;
  margin-left: 2.5%;
}
image{
  height: 100%;
  width: 100%;
}
.user-name{
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  line-height:7.5vw;
  height: 7.5vw;
  width: 50%;
  margin-left: 2.5%;
}
.vedio-time{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 25%;
  margin: 0 2.5%; 
  font-size: 24rpx;
  color:#bfbfbf;
}

.power{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 25%;  
  margin-left: 2%;
  font-size: 24rpx;
  color:#bfbfbf;
}

.charimas{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 25%;  
  margin-left: 2%;
  font-size: 28rpx;
  color:#bfbfbf;
}

.comment-button{
  height: 7.5vw;
  width: 7.5%; 
  margin:0 2.5%; 
}



.foot-bar{
  position: fixed;
  bottom: 0;
  border-top:1px solid #f7f7f7;
  height: 50px;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between ;
}

.foot-item{
  text-align: center;
  flex:1;
  margin-bottom:5px;
}
.foot-item view{
  width: 100%;
}
.foot-text{
  height: 13px;
  line-height: 13px;
  font-size: 12px;
  color: #bfbfbf; 
}
.foot-icon{
  height: 32px;
  line-height:32px;
  font-size: 25px;
  width: 100%;
}



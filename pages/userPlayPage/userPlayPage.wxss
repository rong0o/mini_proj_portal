/**index.wxss**/
/*字体*/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fenxiang:before { content: "\e646"; }

.icon-dianzan:before { content: "\e668"; }

.icon-huatong:before { content: "\e655"; }

.icon-shoucang:before { content: "\e660"; }



/*页面样式*/
button{
  border: none;
  background: white;
}
 button::after{ border: none; }
.vedio-wrap{
  /* height: 300px;  */
  width: 100%;
  border-bottom: 1px solid #f7f7f7;
}
video{
  width: 100%;
}
.clear-float::after{
  content: "";
  height: 0;
  display: block;
  visibility: hiden;
  clear: both;
}
.video-info-wrap{
  width: 100%;
  margin-top: 3px;
}

.user-image,.user-name,.vedio-time,.power,.charimas,.comment-button{
  float: left;
}
.user-image{
  /* height: 80rpx;
  width: 80rpx;
  border-radius: 25rpx; */
  width: 15%;
  height: 15vw;
  margin-left: 2.5%;
  overflow: hidden;
  border-radius: 7.5vw;
}
image{
  height: 100%;
  width: 100%;
}
.user-name{
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  line-height:7.5vw;
  height: 7.5vw;
  width: 49%;
  margin-left: 2%;
}
.vedio-time{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 25%;
  margin: 0 2.5%; 
  font-size: 24rpx;
  color:#bfbfbf;
}

.power{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 23%;  
  margin-left: 2%;
  font-size: 28rpx;
  color:#bfbfbf;
}

.charimas{
  height: 7.5vw;
  line-height: 7.5vw;
  width: 23%;  
  margin-left: 2%;
  font-size: 28rpx;
  color:#bfbfbf;
}
.vedio-icon{
  float:right;
  text-align: venter;
  width: 30%;
  height: 7.5vw;
  line-height: 7.5vw;
  font-size: 7vw;
}
.icon-dianzan{
  float:left;
  height: 7.5vw;
  width: 7.5%; 
  font-size: 7.5vw;
  line-height: 7.5vw;
  margin-left: 2.5%;
  margin-top:-0.5vw;
}
.comment-button{
  height: 7.5vw;
  width: 7.5%; 
  margin:0 2.5%; 
}
.comment-input{
  max-height: 300px;
  width: 99%;
  margin-left: 0.5%;
  border:1px solid #f7f7f7;
  
}
.comment-textarea{
  max-height: 250px;
  width: 100%;
  border-bottom: 1px solid #f7f7f7;
}
.form-btn{
  float: right;
  width: 20%;
  height: 25px;
  font-size: 30rpx;
  line-height: 25px;
  margin:3% 5% 3% 0;
  border: 0;
  background-color: #B5EDE1;
}
.comment-disp{
  margin: 2.5%;
}
.scroll{
  height: 350px;
}
.comment-item{
  width: 100%;
  margin: 10px 0;
}
.comment-user-img,.comment-user-name,.comment-time,.comment-content{
  float: left;
}
.comment-user-img{
  height: 10vw;
  width: 10%;
  margin-right:3%; 
  overflow:hidden;
  border-radius:5vw;
}
.comment-user-name{
  width: 85%;
  height: 5vw;
  line-height: 5vw;
}
.comment-time{
  width: 85%;
  line-height: 5vw;
  font-size: 24rpx;
  color: #bfbfbf;
}
.comment-content{
  float: right;
  width: 87%;
  margin-top:2vw;
}
.foot-bar{
  position: fixed;
  bottom: 0;
  border-top:1px solid #f7f7f7;
  height: 50px;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between ;
  background-color:white;
}

.foot-item{
  text-align: center;
  flex:1;
  margin-bottom:5px;
}
.foot-item view{
  width: 100%;
}
.foot-text{
  height: 13px;
  line-height: 13px;
  font-size: 12px;
  /* color: #bfbfbf;  */
}
.foot-icon{
  height: 32px;
  line-height:32px;
  font-size: 25px;
  width: 100%;
}
.foot-icon image{
  width: 25px;
  height: 27px;
}
.icon-fenxiang{
  color:#B5EDE1;
}


